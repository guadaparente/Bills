SELECT DISTINCT 
ba.external_id as store_address_id,
ii.recipient_fiscal_info,
ifd.tax_id,
billing_cycle

from bls_bills bb
join bls_actors ba on bb.actor_id = ba.id
join inv_invoices ii on ii.bill_id = bb.id
join inv_invoice_items iii on ii.id = iii.invoice_id
join inv_fiscal_details ifd on ba.external_id = ifd.actor_external_id
where ba.partner_family = 'STANDARD'
and ifd.actor_type = 'PARTNER'
and ba.country_code = 'ES'
-- AND ifd.tax_id = 'B88327853,888327853,B88327803,B88327863,B888327853'
and ba.external_id in (646016,646000,688241,646003,653903,235051,391330,510125,658295,658286,658273,658290,222320,37418,598787,222321,235546,27502,598786,222677,235563,92609,598788,635207,315794,198705,199310,315792,102042,598790,635190,755551,222642,197274,598813,237075,636303,222690,144068,598798,236677,243814,291581,241559,598791,688204,303006,247960,291582,246911,598792,247962,303009,255161,291583,255154,598793,254859,303011,255161,254859,268727,268732,268735,291585,303013,755546,755561,755564,755568,755570,268735,315901,315789,688203,315782,635202,315887,315798,315786,315799,315790,598820,315793,315780,315795,315791,688201,635184,315783,315733,315785,315802,598825,598824,315797,315796,460704,598832,315788,350989,350971,598829,350964,357424,357427,598830,357350,339375,384387,384361,598807,384345,384491,598808,447770,386244,222660,386719,385111,598809,216771,598804,386252,237071,387122,387132,598831,387136,464665,464663,339407,508917,510121,598818,339410,646013,646018,646004,646009,690191,690210,689140,690201,738825,739163,732968,738880
)
